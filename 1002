import java.util.Scanner;
/** Calculating the area of a circle */
public class Main{
    public static void main(String args[])
    {
     Scanner in = new Scanner(System.in);  
     double x = in.nextDouble();
     in.close();
     double R = Math.pow(x,2);
     double A = Math.PI * R;
     System.out.printf("A=%.4f", A);
     System.out.println();
    }
}
