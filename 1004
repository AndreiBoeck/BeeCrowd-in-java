import java.util.Scanner;
/** product of two numbers */

public class Main{
    public static void main(String args[])
    {
      Scanner in = new Scanner(System.in);  
      int x = in.nextInt();
      int y = in.nextInt();
      int PROD = x * y;
      System.out.printf("PROD = " + PROD + "\n");
    }
}
