import java.util.Scanner;
/** Area of a sphere */

public class Main{
    public static void main(String args[])
    {
        Scanner in = new Scanner(System.in);
        double R = in.nextDouble();
        double R3 = Math.pow(R, 3);
        double volume = (4.0/3) * 3.14159 * R3;
        System.out.printf("VOLUME = %.3f", volume);
        System.out.println();
    }
}
